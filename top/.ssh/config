Host github.com
Hostname github.com
User git
IdentityFile ~/.ssh/github.com.id
ForwardX11 no

Host iPhone-root
Hostname localhost
Port 32790
UserKnownHostsFile /dev/null
User root
StrictHostKeyChecking no
IdentityFile ~/.ssh/iPhone-root.id

Host iPhone-mobile
Hostname localhost
Port 32790
UserKnownHostsFile /dev/null
User mobile
StrictHostKeyChecking no
IdentityFile ~/.ssh/iPhone-mobile.id

Host *
ForwardX11 yes
ForwardAgent yes
ServerAliveInterval 10
